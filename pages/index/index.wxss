.search_group {
  display: flex;
  flex-direction: row;
  width: 750rpx;
  height: 100rpx;
  background-color: var(--themeColor);
  text-align: center;
}
.search_group .search_icon {
  flex: 1;
  padding-top: 12rpx;
}
.search_group .search_icon image {
  width: 50%;
}
.search_group .search_input {
  padding-top: 12rpx;
  flex: 5;
  height: 80rpx;
}
.search_group .search_input navigator {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 40rpx;
  border: solid #72b782 5rpx;
  color: #666;
}
.search_group .search_input navigator image {
  margin-left: 10rpx;
  width: 7%;
}
.search_group .search_input navigator .search_text {
  flex: 1;
}
.search_group .cart_wrap {
  flex: 1;
}
.search_group .cart_wrap image {
  margin-top: 10rpx;
  width: 40%;
}
.search_group .cart_wrap view {
  font-size: 20rpx;
  color: #000;
}
.index_swiper {
  margin: 10rpx;
  border-radius: 30rpx;
  border: solid whitesmoke 5rpx;
}
.index_swiper swiper {
  width: 730rpx;
  height: 370rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.index_cate navigator {
  width: 25%;
  padding: 20rpx;
  text-align: center;
}
.index_cate navigator image {
  width: 70%;
}
.my_tabs {
  margin: 10rpx;
  border-radius: 30rpx;
  border: solid whitesmoke 5rpx;
}
.my_tabs .first_tab {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.my_tabs .first_tab .goods_item {
  margin-top: 15rpx;
  width: 50%;
  border-bottom: 1px solid #ccc;
  text-align: center;
}
.my_tabs .first_tab .goods_item .goods_group {
  display: flex;
  flex-direction: column;
}
.my_tabs .first_tab .goods_item .goods_group image {
  margin: auto;
  width: 80%;
  border-radius: 25rpx;
}
.my_tabs .first_tab .goods_item .goods_group .goods_name {
  margin-top: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.my_tabs .first_tab .goods_item .goods_group .goods_price {
  padding-left: 30rpx;
  text-align: left;
  color: var(--themeColor);
  font-size: 32rpx;
}
.index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_list {
  overflow: hidden;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
  /* 后四个超链接 */
  /* 2 3 两个超链接 */
}
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  /* 原图的宽高 232 *386 */
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator:nth-child(2),
.index_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
